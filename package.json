{
  "name": "teste-ci-cd",
  "version": "1.0.0",
  "description": "Modelo de automação simples para exemplo de execução no CI do GitHub Actions",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run --browser chrome",
    "allure:serve": "npx allure-commandline serve allure-results",
    "allure:generate": "npx allure-commandline generate --clean allure-results -o allure-report",
    "report": "npm run allure:generate && npx allure-commandline open allure-report",
    "report-ci": "npx allure-commandline generate --clean allure-results -o allure-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "allure-commandline": "^2.34.1",
    "cypress": "^14.2.1"
  },
  "devDependencies": {
    "allure-cypress": "^3.4.1"
  }
}
